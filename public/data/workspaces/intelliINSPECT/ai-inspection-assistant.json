{
  "id": "intelliINSPECT/ai-inspection-assistant",
  "title": "AI Inspection Assistant",
  "description": "AI-powered inspection assistant for equipment assessment and documentation",
  "layout": "dashboard",
  "settings": {
    "autoRefresh": false,
    "theme": "light",
    "density": "comfortable"
  },
  "gadgets": [
    {
      "id": "ai-inspection-assistant-gadget",
      "type": "ai-analysis-wizard-gadget",
      "title": "AI Inspection Assistant",
      "description": "Comprehensive inspection wizard with AI-powered analysis and form population",
      "config": {
        "sections": [
          {
            "id": "voice_capture",
            "title": "Voice Capture",
            "description": "Record inspection notes and observations. Transcription will feed following steps.",
            "sectionType": "voice",
            "includeInPdf": false
          },
          {
            "id": "nameplate_capture",
            "title": "Equipment Images",
            "description": "Upload images of equipment nameplates and vessel conditions for AI analysis.",
            "sectionType": "image",
            "includeInPdf": false,
            "images": {
              "maxCount": 10,
              "maxSize": 10485760,
              "accept": "image/jpeg,image/png,image/gif,image/webp",
              "multiple": true,
              "showPreview": true,
              "showThumbnails": true,
              "thumbnailSize": 150,
              "drawingEnabled": true,
              "drawingTools": [
                "pen",
                "line",
                "circle",
                "rectangle",
                "text",
                "arrow"
              ],
              "drawingColors": [
                "#FF0000",
                "#00FF00",
                "#0000FF",
                "#FFFF00",
                "#FF00FF",
                "#00FFFF"
              ],
              "strokeWidths": [
                1,
                2,
                3,
                5,
                8
              ],
              "uploadText": "Click or drag images here to upload",
              "dragText": "Drop images here to upload"
            },
            "required": false,
            "disabled": false,
            "gridLayout": {
              "xs": 24,
              "sm": 24,
              "md": 24,
              "lg": 24
            }
          },
          {
            "id": "image_analysis",
            "title": "Equipment Analysis",
            "description": "Detailed AI analysis of captured images including nameplate data and vessel conditions. The AI will automatically detect the equipment type and show only relevant assessment modules.",
            "imageAnalysisPromptRef": "detailedImageAnalysis",
            "promptSourceIds": [
              "nameplate_capture"
            ],
            "includeInPdf": true,
            "analysisWidget": "simple",
            "pdf": {
              "includeOverview": false,
              "includeImages": true,
              "imagesDataPath": "sections[nameplate_capture].images",
              "fit": "contain",
              "align": "center"
            }
          },
          {
            "id": "general_information",
            "title": "General Information",
            "description": "Common inspection information for all equipment types.",
            "sectionType": "form",
            "pdfLayout": {
              "type": "grid",
              "columns": 3,
              "labelPosition": "top"
            },
            "btnLabel": "Populate General Info",
            "promptRef": "generalInfoPopulate",
            "includeInPdf": true
          },
          {
            "id": "inspection_scope",
            "title": "Inspection Scope & Methodology",
            "description": "Scope description and inspection methods used.",
            "sectionType": "form",
            "pdfLayout": {
              "type": "grid",
              "columns": 2,
              "labelPosition": "top"
            },
            "btnLabel": "Populate Scope",
            "promptRef": "scopePopulate",
            "includeInPdf": true
          },
          {
            "id": "condition_assessment_vessels",
            "title": "3A - Pressure Vessel Condition Assessment",
            "description": "Document pressure vessel conditions and findings.",
            "includeInPdf": true,
            "watchField": "detected_equipment_type",
            "showWhen": "pressure_vessel",
            "grid": {
              "title": "Pressure Vessel Findings",
              "dataKey": "pvFindings",
              "columns": [
                {
                  "key": "location",
                  "title": "Location/Component",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "category",
                  "title": "Category",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "finding",
                  "title": "Finding/Anomaly",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "severity",
                  "title": "Severity",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "measurements",
                  "title": "Measurements",
                  "editable": true
                },
                {
                  "key": "recommendations",
                  "title": "Recommendations",
                  "editable": true
                }
              ],
              "promptRef": "pvConditionPopulate",
              "btnLabel": "Populate PV Findings"
            }
          },
          {
            "id": "condition_assessment_piping",
            "title": "3B - Piping Condition Assessment",
            "description": "Document piping system conditions and findings.",
            "includeInPdf": true,
            "watchField": "detected_equipment_type",
            "showWhen": "piping",
            "grid": {
              "title": "Piping System Findings",
              "dataKey": "pipingFindings",
              "columns": [
                {
                  "key": "location",
                  "title": "Location/Line",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "category",
                  "title": "Category",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "finding",
                  "title": "Finding/Anomaly",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "severity",
                  "title": "Severity",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "cmlReading",
                  "title": "CML Reading",
                  "editable": true
                },
                {
                  "key": "recommendations",
                  "title": "Recommendations",
                  "editable": true
                }
              ],
              "promptRef": "pipingConditionPopulate",
              "btnLabel": "Populate Piping Findings"
            }
          },
          {
            "collapsed": true,
            "id": "condition_assessment_tanks",
            "title": "3C - Storage Tank Condition Assessment",
            "description": "Document storage tank conditions and findings.",
            "includeInPdf": true,
            "watchField": "detected_equipment_type",
            "showWhen": "storage_tank",
            "grid": {
              "title": "Storage Tank Findings",
              "dataKey": "tankFindings",
              "columns": [
                {
                  "key": "location",
                  "title": "Location/Component",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "category",
                  "title": "Category",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "finding",
                  "title": "Finding/Anomaly",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "severity",
                  "title": "Severity",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "measurements",
                  "title": "Measurements",
                  "editable": true
                },
                {
                  "key": "recommendations",
                  "title": "Recommendations",
                  "editable": true
                }
              ],
              "promptRef": "tankConditionPopulate",
              "btnLabel": "Populate Tank Findings"
            }
          },
          {
            "id": "condition_assessment_relief",
            "title": "3D - Relief Device Condition Assessment",
            "description": "Document pressure relief device conditions and findings.",
            "includeInPdf": true,
            "watchField": "detected_equipment_type",
            "showWhen": "relief_device",
            "grid": {
              "title": "Relief Device Findings",
              "dataKey": "reliefFindings",
              "columns": [
                {
                  "key": "deviceTag",
                  "title": "Device Tag",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "category",
                  "title": "Category",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "finding",
                  "title": "Finding/Anomaly",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "testResult",
                  "title": "Test Result",
                  "editable": true
                },
                {
                  "key": "setPoint",
                  "title": "Set Point",
                  "editable": true
                },
                {
                  "key": "recommendations",
                  "title": "Recommendations",
                  "editable": true
                }
              ],
              "promptRef": "reliefConditionPopulate",
              "btnLabel": "Populate Relief Device Findings"
            }
          },
          {
            "id": "condition_assessment_special",
            "title": "3E - Special Programs Assessment",
            "description": "Document special program findings (PMI, CP, lining, etc.).",
            "includeInPdf": true,
            "grid": {
              "title": "Special Program Findings",
              "dataKey": "specialFindings",
              "columns": [
                {
                  "key": "program",
                  "title": "Program Type",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "location",
                  "title": "Location",
                  "editable": true
                },
                {
                  "key": "finding",
                  "title": "Finding/Result",
                  "editable": true
                },
                {
                  "key": "compliance",
                  "title": "Compliance Status",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "recommendations",
                  "title": "Recommendations",
                  "editable": true
                }
              ],
              "promptRef": "specialConditionPopulate",
              "btnLabel": "Populate Special Program Findings"
            }
          },
          {
            "id": "thickness_measurements",
            "title": "4 - Thickness Measurement & Calculations",
            "description": "CML readings and thickness calculations.",
            "includeInPdf": true,
            "grid": {
              "title": "Thickness Measurements",
              "dataKey": "measurements",
              "columns": [
                {
                  "key": "location",
                  "title": "Location",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "nominalThickness",
                  "title": "Nominal Thickness",
                  "editable": true,
                  "summary": "average"
                },
                {
                  "key": "actualThickness",
                  "title": "Actual Thickness",
                  "editable": true,
                  "summary": "average"
                },
                {
                  "key": "corrosionRate",
                  "title": "Corrosion Rate",
                  "editable": true,
                  "summary": "average"
                },
                {
                  "key": "remainingLife",
                  "title": "Remaining Life (years)",
                  "editable": true,
                  "summary": "minimum"
                }
              ],
              "promptRef": "thicknessPopulate",
              "btnLabel": "Populate Measurements"
            }
          },
          {
            "id": "evaluation_compliance",
            "title": "5 - Evaluation & Compliance",
            "description": "Fitness-for-service and code compliance assessment.",
            "sectionType": "form",
            "pdfLayout": {
              "type": "grid",
              "columns": 1,
              "labelPosition": "top"
            },
            "btnLabel": "Populate Evaluation",
            "promptRef": "evaluationPopulate",
            "includeInPdf": true
          },
          {
            "id": "recommendations",
            "title": "6 - Recommendations",
            "description": "Required repairs, alterations, and improvements.",
            "includeInPdf": true,
            "grid": {
              "title": "Recommendations",
              "dataKey": "recommendations",
              "columns": [
                {
                  "key": "priority",
                  "title": "Priority",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "type",
                  "title": "Type",
                  "editable": true,
                  "summary": "uniqueCount"
                },
                {
                  "key": "description",
                  "title": "Description",
                  "editable": true,
                  "summary": "countMissing"
                },
                {
                  "key": "method",
                  "title": "Method/Standard",
                  "editable": true
                },
                {
                  "key": "timeline",
                  "title": "Timeline",
                  "editable": true
                }
              ],
              "promptRef": "recommendationsPopulate",
              "btnLabel": "Populate Recommendations"
            }
          },
          {
            "id": "attachments_section",
            "title": "7 - Attachments",
            "description": "Supporting documentation and records.",
            "sectionType": "form",
            "pdfLayout": {
              "type": "grid",
              "columns": 1,
              "labelPosition": "top"
            },
            "includeInPdf": true
          },
          {
            "id": "inspector_signoff",
            "title": "Inspector Signoff",
            "description": "Inspector certification and approval.",
            "sectionType": "form",
            "pdfLayout": {
              "type": "grid",
              "columns": 2,
              "labelPosition": "top"
            },
            "includeInPdf": true
          }
        ],
        "groups": [
          {
            "id": "inspection_details",
            "title": "Inspection Details",
            "sectionId": "general_information",
            "lgSpan": 24
          },
          {
            "id": "equipment_details",
            "title": "Equipment Details",
            "sectionId": "general_information",
            "lgSpan": 24
          },
          {
            "id": "design_data",
            "title": "Design Data",
            "sectionId": "general_information",
            "lgSpan": 24
          },
          {
            "id": "scope_details",
            "title": "Scope Details",
            "sectionId": "inspection_scope",
            "lgSpan": 24
          },
          {
            "id": "nde_methods",
            "title": "NDE Methods",
            "sectionId": "inspection_scope",
            "lgSpan": 24
          },
          {
            "id": "calculations",
            "title": "Calculations",
            "sectionId": "evaluation_compliance",
            "lgSpan": 24
          },
          {
            "id": "compliance_assessment",
            "title": "Compliance Assessment",
            "sectionId": "evaluation_compliance",
            "lgSpan": 24
          },
          {
            "id": "attachment_list",
            "title": "Attachment List",
            "sectionId": "attachments_section",
            "lgSpan": 24
          },
          {
            "id": "inspector_info",
            "title": "Inspector Information",
            "sectionId": "inspector_signoff",
            "lgSpan": 24
          }
        ],
        "fields": [
          {
            "id": "detected_equipment_type",
            "label": "Equipment Type",
            "type": "select",
            "readonly": false,
            "groupId": "equipment_details",
            "lgSpan": 24,
            "placeholder": "Select or auto-detect equipment type",
            "options": [
              {
                "label": "Pressure Vessel",
                "value": "pressure_vessel"
              },
              {
                "label": "Piping System",
                "value": "piping"
              },
              {
                "label": "Storage Tank",
                "value": "storage_tank"
              },
              {
                "label": "Relief Device",
                "value": "relief_device"
              },
              {
                "label": "Heat Exchanger",
                "value": "heat_exchanger"
              },
              {
                "label": "Other",
                "value": "other"
              }
            ]
          },
          {
            "id": "inspection_type",
            "label": "Inspection Type",
            "type": "dropdown",
            "options": [
              "External",
              "Internal",
              "On-stream",
              "RBI",
              "Turnaround",
              "Post-repair",
              "Post-alteration",
              "Pre-service"
            ],
            "required": true,
            "groupId": "inspection_details",
            "lgSpan": 8
          },
          {
            "id": "equipment_id",
