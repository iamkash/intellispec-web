      {
        "id": "inspection_type",
        "type": "multiselect",
        "label": "Inspection Type",
        "placeholder": "Select inspection types...",
        "optionsUrl": "/api/options/wizard?distinct=identity.domainSubType&identity.domain=inspection&includeFields=identity.domain,identity.domainLabel,identity.domainSubTypeLabel,workspaceId&labelField=identity.domainSubTypeLabel",
        "labelField": "label",
        "valueField": "value",
        "refreshTrigger": true
      },
      {
        "id": "status",
        "type": "multiselect",
        "label": "Status",
        "placeholder": "Select status...",
        "optionsUrl": "/api/options/wizard?distinct=status&identity.domain=inspection&includeFields=identity.domain,identity.domainLabel,identity.domainSubTypeLabel,workspaceId&labelField=identity.domainSubTypeLabel",
        "labelField": "label",
        "valueField": "value",
        "refreshTrigger": true
      },
      {
        "id": "date_range",
        "type": "daterange",
        "label": "Date Range",
        "placeholder": "Select date range...",
